{
  "schema_version": 1,
  "quilt_loader": {
    "group": "cf.witcheskitchen",
    "id": "witcheskitchen",
    "version": "${version}",
    "metadata": {
      "name": "Witches' Kitchen",
      "description": "A cottagecore-themed magic mod. Addons are planned!",
      "authors": {
        "DanikingRD": "Author",
        "MrSterner": "Author"
      },
      "contact": {
        "homepage": "https://github.com/witches-kitchen/WK",
        "sources": "https://github.com/witches-kitchen/WK",
        "issues": "https://github.com/witches-kitchen/WK/issues"
      },
      "icon": "assets/witcheskitchen/icon.png",
      "license": "MIT"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": "cf.witcheskitchen.WitchesKitchen",
      "client_init": "cf.witcheskitchen.client.WitchesKitchenClient",
      "rei_client": "cf.witcheskitchen.integration.client.rei.WKREIPlugin",
      "modmenu": "cf.witcheskitchen.integration.client.WKModMenu",
