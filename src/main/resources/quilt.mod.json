    "depends": [
      {
        "id": "geckolib3",
        "versions": ">=3.1.6"
      },
      {
        "id": "trinkets",
        "versions": ">=3.4.0"
      },
      {
        "id": "quilt_loader",
        "versions": ">=0.17.0-"
      },
      {
        "id": "quilted_fabric_api",
        "versions": ">=4.0.0-"
      },
      {
        "id": "minecraft",
        "versions": ">=1.19.2"
      }
    ]
  },
  "access_widener": "wk.accesswidener",
  "mixin": "wk.mixins.json",
  "cardinal-components": [
    "witcheskitchen:curse",
    "witcheskitchen:player"
  ]
}